<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:View">

    <Color x:Key="gray">#2B2B2B</Color>
    <SolidColorBrush  x:Key="Gray" Color="{StaticResource gray}" />

    <Color x:Key="darkGray">#1E1E1E</Color>
    <SolidColorBrush  x:Key="DarkGray" Color="{StaticResource darkGray}" />

    <Color x:Key="lightGray">#3C393A</Color>
    <SolidColorBrush  x:Key="LightGray" Color="{StaticResource lightGray}" />

    <Color x:Key="white">#D1D1D1</Color>
    <SolidColorBrush  x:Key="White" Color="{StaticResource white}" />

    <Color x:Key="darkRed">#781C1B</Color>
    <SolidColorBrush  x:Key="DarkRed" Color="{StaticResource darkRed}" />

    <Color x:Key="mediumGray">#383838</Color>
    <SolidColorBrush  x:Key="MediumGray" Color="{StaticResource mediumGray}" />
    

</ResourceDictionary>